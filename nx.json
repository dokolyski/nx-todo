{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "todo-workspace",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "todo-app": { "tags": ["type:application"] },
    "todo-app-e2e": { "tags": [], "implicitDependencies": ["todo-app"] },
    "tasks-feature": { "tags": ["type:feature"] },
    "tasks-data-access": { "tags": ["type:data-access"] },
    "tasks-ui-task-form-dialog": { "tags": ["type:ui"] },
    "tasks-ui-tasks-list": { "tags": ["type:ui"] }
  }
}
