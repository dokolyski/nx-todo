{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "todo-app",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "todo-app": {
      "projectType": "application",
      "root": "apps/todo-app",
      "sourceRoot": "apps/todo-app/src",
      "prefix": "todo-workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todo-app",
            "index": "apps/todo-app/src/index.html",
            "main": "apps/todo-app/src/main.ts",
            "polyfills": "apps/todo-app/src/polyfills.ts",
            "tsConfig": "apps/todo-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/todo-app/src/favicon.ico",
              "apps/todo-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/todo-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todo-app/src/environments/environment.ts",
                  "with": "apps/todo-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todo-app:build",
            "proxyConfig": "proxy.config.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "todo-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todo-app/src/**/*.ts",
              "apps/todo-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/todo-app"],
          "options": {
            "jestConfig": "apps/todo-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todo-app-e2e": {
      "root": "apps/todo-app-e2e",
      "sourceRoot": "apps/todo-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todo-app-e2e/cypress.json",
            "tsConfig": "apps/todo-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "todo-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todo-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/todo-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "tasks-feature": {
      "projectType": "library",
      "root": "libs/tasks/feature",
      "sourceRoot": "libs/tasks/feature/src",
      "prefix": "todo-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tasks/feature/src/**/*.ts",
              "libs/tasks/feature/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tasks/feature"],
          "options": {
            "jestConfig": "libs/tasks/feature/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tasks-data-access": {
      "projectType": "library",
      "root": "libs/tasks/data-access",
      "sourceRoot": "libs/tasks/data-access/src",
      "prefix": "todo-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tasks/data-access/src/**/*.ts",
              "libs/tasks/data-access/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tasks/data-access"],
          "options": {
            "jestConfig": "libs/tasks/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tasks-ui-task-form-dialog": {
      "projectType": "library",
      "root": "libs/tasks/ui-task-form-dialog",
      "sourceRoot": "libs/tasks/ui-task-form-dialog/src",
      "prefix": "todo-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tasks/ui-task-form-dialog/src/**/*.ts",
              "libs/tasks/ui-task-form-dialog/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tasks/ui-task-form-dialog"],
          "options": {
            "jestConfig": "libs/tasks/ui-task-form-dialog/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tasks-ui-tasks-list": {
      "projectType": "library",
      "root": "libs/tasks/ui-tasks-list",
      "sourceRoot": "libs/tasks/ui-tasks-list/src",
      "prefix": "todo-workspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tasks/ui-tasks-list/src/**/*.ts",
              "libs/tasks/ui-tasks-list/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tasks/ui-tasks-list"],
          "options": {
            "jestConfig": "libs/tasks/ui-tasks-list/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
